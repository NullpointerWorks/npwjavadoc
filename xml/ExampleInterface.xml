<?xml version="1.0" encoding="UTF-8" ?>
<source>
	<module></module>
	<package>com.nullpointerworks.examples</package>
	<commentary>
		This interface serves as a scan test to see if all aspects of an interface are observed.
		<author>Michiel</author>
	</commentary>
	<code>
		<type>interface</type>
		<name>ExampleInterface</name>
		<field>
			<name>myVariable</name>
			<type>int</type>
			<modifiers>public static final</modifiers>
			<data>0</data>
		</field>
		<method>
			<commentary>public method</commentary>
			<name>myPublicMethod</name>
			<returns>
				<type>void</type>
			</returns>
		</method>
		<method>
			<commentary>abstract method</commentary>
			<name>myPublicMethod2</name>
			<returns>
				<type>void</type>
			</returns>
			<modifiers>abstract</modifiers>
		</method>
		<method>
			<commentary>no modifiers are also public by default</commentary>
			<name>myPublicMethod3</name>
			<returns>
				<type>void</type>
			</returns>
			<param>
				<type>int</type>
				<name>i</name>
				<comment>testing parameter</comment>
			</param>
		</method>
		<method>
			<commentary>my default method</commentary>
			<name>myDefaultMethod</name>
			<returns>
				<type>void</type>
			</returns>
			<modifiers>default</modifiers>
		</method>
		<method>
			<name>myDefaultMethod2</name>
			<returns>
				<type>void</type>
			</returns>
			<modifiers>default</modifiers>
			<param>
				<type>int</type>
				<name>i</name>
			</param>
			<param>
				<type>int</type>
				<name>j</name>
			</param>
		</method>
		<method>
			<commentary>my default method with a return type</commentary>
			<name>myDefaultMethod3</name>
			<returns>
				<type>String</type>
			</returns>
			<modifiers>default</modifiers>
		</method>
		<method>
			<commentary>my default strict floating-point method with a return type</commentary>
			<name>myDefaultMethod4</name>
			<returns>
				<type>String</type>
			</returns>
			<modifiers>default strictfp</modifiers>
		</method>
	</code>
</source>
